{
    "name": "emath.js",
    "version": "9.6.0",
    "description": "A library for incremental games based on break_eternity.js",
    "homepage": "https://github.com/xShadowBlade/emath.js#readme",
    "bugs": {
        "url": "https://github.com/xShadowBlade/emath.js/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xShadowBlade/emath.js.git"
    },
    "license": "MIT",
    "author": "xShadowBlade",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "default": "./dist/main/eMath.js"
        },
        "./game": {
            "types": "./dist/types/game/index.d.ts",
            "default": "./dist/game/eMath.game.js"
        },
        "./presets": {
            "types": "./dist/types/presets/index.d.ts",
            "default": "./dist/presets/eMath.presets.js"
        },
        "./ts": {
            "default": "./src/index.ts"
        },
        "./ts/game": {
            "default": "./src/game/index.ts"
        },
        "./ts/presets": {
            "default": "./src/presets/index.ts"
        }
    },
    "scripts": {
        "rollup": "rollup -c rollup.config.mjs",
        "build": "concurrently \"rollup -c rollup.config.mjs\" \"tsc\"",
        "build-and-build-and-test": "concurrently \"rollup -c rollup.config.mjs\" \"cd test && npm run build && cd ..\" && cd test && npm run test && cd ..",
        "build-and-test": "rollup -c rollup.config.mjs && cd test && npm run test && cd ..",
        "build:main": "npm run rollup",
        "build:types": "tsc",
        "doc": "typedoc --options typedoc.json",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint --fix src/**/*.ts",
        "test": "cd test && npm run test && cd .."
    },
    "dependencies": {
        "class-transformer": "^0.5.1",
        "reflect-metadata": "^0.2.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@rollup/plugin-commonjs": "^28.0.1",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@rollup/plugin-swc": "^0.4.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@swc/cli": "^0.5.0",
        "@swc/core": "^1.9.2",
        "@types/eslint__js": "^8.42.3",
        "@types/md5": "^2.3.5",
        "@types/node": "^22.0.0",
        "concurrently": "^9.0.0",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsdoc": "^50.2.2",
        "eslint-plugin-prettier": "^5.2.1",
        "lz-string": "^1.5.0",
        "md5": "^2.3.0",
        "prettier": "^3.3.3",
        "rollup": "^4.27.2",
        "tslib": "^2.8.1",
        "typedoc": "^0.26.5",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.4.0"
    }
}
