{
    "name": "emath.js",
    "version": "1.0.0",
    "description": "A math library for incremental games based on break_eternity.js",
    "main": "dist/eMath.js",
    "unpkg": "dist/eMath.min.js",
    "scripts": {
        "build": "bili",
        "build-bwy": "browserify src/index.js -o dist/bundle.js",
        "build-esb": "esbuild src/index.js --bundle --format=iife --outfile=dist/out.js",
        "test": "mocha"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xShadowBlade/emath.js.git"
    },
    "author": "xShadowBlade",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/xShadowBlade/emath.js/issues"
    },
    "homepage": "https://github.com/xShadowBlade/emath.js#readme",
    "dependencies": {
        "break_eternity.js": "^1.3.1",
        "browserify": "^17.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^5.0.0",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.2.1",
        "bili": "^5.0.5",
        "esbuild": "0.19.4",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "mocha": "^10.2.0",
        "prettier": "3.0.3",
        "rollup": "^2.79.1",
        "rollup-plugin-babel": "^4.4.0"
    }
}
