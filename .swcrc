{
    "$schema": "https://swc.rs/schema.json",
    "jsc": {
        "minify": {
            "compress": {
                "global_defs": {
                    "PKG_VERSION": "9.6.0"
                }
            }
        },
        "target": "es2017",
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "dynamicImport": true,
            "decorators": true,
            "importMeta": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        }
    },
    "module": {
        "type": "es6"
    }
}
